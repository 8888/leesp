; atoms
(def {nil} {})
(def {true} 1)
(def {false} 0)

; function definitions
(def {func} (\ {f b} {
  def (head f) (\ (tail f) b)
}))

; unpack list for function
(func {unpack f x} {
  eval (join (list f) x)
})

; pack list for function
(func {pack f & xs} {f xs})

; curried and uncurried alias calling
(def {curry} unpack)
(def {uncurry} pack)
